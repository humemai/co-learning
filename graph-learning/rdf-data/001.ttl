@prefix co_learning: <http://example.org/co_learning#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

co_learning:CP1 a co_learning:CollaborationPattern ;
    co_learning:hasActionHuman co_learning:ActionHuman1_0 ;
    co_learning:hasActionRobot co_learning:ActionRobot1_0 ;
    co_learning:hasCPLabel "Small rocks on top"^^xsd:string ;
    co_learning:hasCPNum 1 ;
    co_learning:hasParticipantNumber 4073 ;
    co_learning:hasRoundNumber 2 ;
    co_learning:hasSituation co_learning:Situation1_0 ;
    co_learning:hasTicksLasted 2470 ;
    co_learning:hasTimeScore 6400 .

co_learning:ActionHuman1_0 a co_learning:ActionHuman ;
    co_learning:hasNextActionHuman co_learning:ActionHuman1_1 .

co_learning:ActionHuman1_1 a co_learning:ActionHuman ;
    co_learning:hasAction "Pick up <object> in <location>" ;
    co_learning:hasLocation "Top of rock pile" ;
    co_learning:hasNextActionHuman co_learning:ActionHuman1_2 ;
    co_learning:hasObject "Small rock" .

co_learning:ActionHuman1_2 a co_learning:ActionHuman ;
