    co_learning:hasLocation "<Left> side of field" ;
    co_learning:hasObject "Small rock" .

co_learning:ActionRobot1_0 a co_learning:ActionRobot ;
    co_learning:hasAction "Stand still in <location>" ;
    co_learning:hasLocation "<Right> side of field" ;
    co_learning:hasNextActionRobot co_learning:ActionRobot1_1 .

co_learning:ActionRobot1_1 a co_learning:ActionRobot ;
    co_learning:hasNextActionRobot co_learning:ActionRobot1_2 .

co_learning:ActionRobot1_2 a co_learning:ActionRobot .

co_learning:Situation1_0 a co_learning:Situation ;
    co_learning:hasLocation "Top of rock pile" ;
    co_learning:hasNextSituation co_learning:Situation1_1 ;
    co_learning:hasObject "Small rock" .

co_learning:Situation1_1 a co_learning:Situation .

